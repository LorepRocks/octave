			</div>
			<a class="ico icon-1 pc_usuario_mas"></a>
		</div>
		<div class="boton">
			<button class="bt_usuario_mas">{{'MORE_USERS' | translate}}</button>
			<br>
			<a href="/logout"><button><b class="ico icon-19"></b>{{'LOG_OUT' | translate}}</button></a>
		</div>
		<a  class="ico icon-1 cerrar_cambio"></a>
	</div>
	</body>

	<script type="text/javascript">
		function switchMenu(menu){
			$(".active").removeClass("active");
			menu.className = "active";			
		}
	</script>
	<script type="text/javascript">
		$( ".principal" ).click(function() {
			$(".principal").toggleClass("active" );
			$(".titulo").toggleClass("display" );
			$(".menu_lista").toggleClass("display" );
			$(".container").toggleClass("active" );
			$(".ico-menu").toggleClass(" icon_12" );
		});
	</script>
	<script type="text/javascript">
		$( ".cambiar" ).click(function() {
			$(".cambio_usuarios").toggleClass("display" );
		});
		$( ".cerrar_cambio" ).click(function() {
			$(".cambio_usuarios").toggleClass("display" );
		});
	</script>


