			<div class="cambio_usuario">
				<b class="ico icon-4 cambiar"></b>
			</div>
		</footer>
	</div>
	<div class="cambio_usuarios display">
		<div class="usuario uprincipal">
			<div class="foto"><img src="img/e1.jpg"></div>
			<div id="user" class="user"><p><%=userJSON.username%></p></div>
		</div>
		<div class="usuarios">
			<div class="usuario">
				<div class="foto"><img src="img/e1.jpg"></div>
				<div id="user" class="user"><p><%=userJSON.username%></p></div>
